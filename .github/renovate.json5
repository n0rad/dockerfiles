{
  extends: [
    "config:base",
    "regexManagers:dockerfileVersions",
  ],

  automerge: true,
  automergeType: "branch",
  packageRules: [
    {
      matchDatasources: ["docker"],
      matchPackagePrefixes: ["n0rad/"],
      versioning: "regex:^(?<major>\\d+)(\\.(?<minor>\\d+))?(\\.(?<patch>\\d+))?(-(?<build>.*))?$",
    }
  ],
}
